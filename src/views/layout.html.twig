<!DOCTYPE html>
<html lang="en-us">
<head>
    <meta charset="utf-8">
    <title>mongoqp</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="{{ global.request.basepath }}/css/bootstrap.min.css" rel="stylesheet" />
    <link href="{{ global.request.basepath }}/css/bootstrap-responsive.min.css" rel="stylesheet" />
    <link href="{{ global.request.basepath }}/css/main.css" rel="stylesheet" />
</head>
<body>
    <div class="container">
        <div class="page-header">
            <div class="row-fluid">
                <div class="span6"><h1>mongoqp: <small>a query profile viewer</small></h1></div>
                <div class="span6 connection"><i class="icon-leaf"></i> {{ app["query.profiler"].primaryHostAndPort }}</div>
            </div>
        </div>
        {% block content '' %}
    </div>
    <script src="{{ global.request.basepath }}/js/jquery-1.8.1.min.js"></script>
    <script src="{{ global.request.basepath }}/js/bootstrap.min.js"></script>
    <script src="{{ global.request.basepath }}/js/jquery.dataTables.min.js"></script>
    <script src="{{ global.request.basepath }}/js/jquery.dataTables.bootstrap.paging.js"></script>
    <script src="{{ global.request.basepath }}/js/main.js"></script>
</body>
</html>
